input {
	relp {
		add_field => [ "_logstash_input", "relp" ]
		host => "0.0.0.0"
		port => "<%= p("logstash_ingestor.relp.port") %>"
	}
}
			
output {
	<% if p("logstash_ingestor.debug") %>
        stdout {
            codec => "json"
        }
	<% end %>

	redis {
		host => "<%= p("redis.host") %>"
		port => "<%= p("redis.port") %>"
		data_type => "list"
		key => "<%= p("redis.key") %>"
		batch => true
		batch_events => 50
	}
}
